{
	"tasks": {
		"build:npm": "deno run --allow-all ./tasks/build-npm.ts",
		"start": "deno run --unstable --allow-read --allow-env --allow-write src/index.ts",
		"fmt": "deno fmt"
	},
	"fmt": {
		"singleQuote": true,
		"lineWidth": 80,
		"semiColons": true,
		"indentWidth": 4,
		"useTabs": true
	}
}
